<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="inherit_view_change_product_quantity" model="ir.ui.view">
        <field name="name">inherit.view.change.product.quantity</field>
        <field name="model">stock.change.product.qty</field>
        <field name="inherit_id" ref="stock.view_change_product_quantity"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='new_quantity']" position="attributes">
                <attribute name="readonly">1</attribute>
            </xpath>
        </field>
    </record>

</odoo>